{
  "name": "datadeck",
  "description": "Provides an Edit in DataDeck link from a hosted gist file on GitHub.",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://gist.github.com/*", "https://github.com/*"],
      "css": ["datadeck.css"],
      "js": ["datadeck.js"],
      "run_at": "document_end"
    }
  ]
}
